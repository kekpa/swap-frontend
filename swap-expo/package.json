{
  "name": "swap",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start --dev-client",
    "start:dev": "NODE_ENV=development expo start --dev-client",
    "start:prod": "NODE_ENV=production expo start --dev-client",
    "android": "expo run:android",
    "android:dev": "NODE_ENV=development expo run:android",
    "android:prod": "NODE_ENV=production expo run:android",
    "ios": "expo run:ios",
    "ios:dev": "NODE_ENV=development expo run:ios",
    "ios:prod": "NODE_ENV=production expo run:ios",
    "web": "expo start --web",
    "web:dev": "NODE_ENV=development expo start --web",
    "web:prod": "NODE_ENV=production expo start --web",
    "prebuild": "expo prebuild --clean",
    "prebuild:clean": "expo prebuild --clean",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "maestro test .maestro/flows/",
    "test:e2e:signup": "maestro test .maestro/flows/signup-flow.yaml",
    "test:e2e:login": "maestro test .maestro/flows/login-flow.yaml",
    "test:e2e:send": "maestro test .maestro/flows/send-money-flow.yaml",
    "test:e2e:critical": "maestro test .maestro/flows/ --include-tags critical",
    "typecheck": "tsc --noEmit",
    "prepare": "husky"
  },
  "dependencies": {
